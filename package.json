{
  "name": "vue-boilerplate",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "npm run type-check && npm run build-only",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --ext .ts,.vue --ignore-path .gitignore . --fix",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit --composite false",
    "gql:gen": "graphql-codegen --require dotenv/config --config codegen.js dotenv_config_path=.env.local"
  },
  "dependencies": {
    "@apollo/client": "3.6.9",
    "@quasar/extras": "1.15.2",
    "axios": "0.27.2",
    "date-fns": "2.29.2",
    "pinia": "2.0.21",
    "quasar": "2.7.7",
    "vue": "3.2.37",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.5"
  },
  "devDependencies": {
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.11.8",
    "@graphql-codegen/introspection": "2.2.1",
    "@graphql-codegen/typescript": "2.7.3",
    "@graphql-codegen/typescript-operations": "2.5.3",
    "@gumper-x/eslint-config": "1.4.8",
    "@gumper-x/prettier-config": "2.0.3",
    "@intlify/vite-plugin-vue-i18n": "6.0.1",
    "@quasar/vite-plugin": "1.1.1",
    "@rollup/plugin-graphql": "1.1.0",
    "@types/node": "18.7.13",
    "@vitejs/plugin-vue": "3.0.3",
    "eslint": "8.23.0",
    "sass": "1.32.12",
    "typescript": "4.8.2",
    "vite": "3.0.9",
    "vite-plugin-checker": "0.5.0",
    "vue-tsc": "0.40.4"
  }
}
